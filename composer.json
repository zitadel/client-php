{
  "name": "zitadel/client",
  "description": "Client library for Zitadel",
  "keywords": [
    "zitadel",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/zitadel/client-php",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Zitadel",
      "homepage": "https://zitadel.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Zitadel\\Client\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zitadel\\Client\\Test\\": "test/"
    }
  }
}
